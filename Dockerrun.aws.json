{"AWSEBDockerrunVersion":2,
"containerDefinitions": [
        {
            "name": "client",
            "image": "nahuelolea2/multi-client",
            "hostname": "client",
            "essential": false
        },
        {
            "name": "server",
            "image": "nahuelolea2/multiserver",
            "hostname": "api",
            "essential": false
        },
        {
            "name": "worker",
            "image": "nahuelolea2/multi-worker",
            "hostname": "worker",
            "essensial": false
        },
        {
            "name": "nginx",
            "image": "nahuelolea2/nginx",
            "hostname": "nginx",
            "essential": true,
            "portsMappings": [
                {
                    "hostPorts": 80,
                    "containerPort": 80
                }
            ],
            "Links": ["client", "server"]
        }
    ]
}